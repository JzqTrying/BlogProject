<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AdminLoginController</servlet-name>
    <servlet-class>tzc.controller.AdminLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminLoginController</servlet-name>
    <url-pattern>/login</url-pattern>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminRegisterController</servlet-name>
    <servlet-class>tzc.controller.AdminRegisterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminRegisterController</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
</web-app>